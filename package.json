{
  "name": "hono-cli-adapter",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "description": "Adapt CLI argv to a POST Request for a Hono app (ESM).",
  "author": "K.Endo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiyo-e/hono-cli-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/kiyo-e/hono-cli-adapter/issues"
  },
  "homepage": "https://github.com/kiyo-e/hono-cli-adapter#readme",
  "keywords": ["hono", "cli", "adapter", "request", "esm"],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "build:example:bin": "npm run build && bun build --compile ./example/basic/cli.mjs --outfile ./example/basic/bin/hono-example"
  },
  "dependencies": {
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.3.0",
    "hono": "^4.9.5",
    "typescript": "^5.5.0"
  }
}
